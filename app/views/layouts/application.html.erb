<!DOCTYPE html>
<html>
  <head>
    <title>RorSocialScaffold</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>
  <body>
    <nav class="nav">

   

   <%= link_to 'Fakebook', root_path %>
      
      <div class="menu">
        <div class="menu-item">
          <%= menu_link_to 'Timeline', posts_path %>
        </div>
        <div class="menu-item">
          <%= menu_link_to 'All users', users_path %>
        </div>
        <div class="menu-item">
          <%= menu_link_to 'Friendships', friendships_path %>
        </div>
      </div>

      <%= nav_bar %>
    </nav>
    <%= flash_notice %>
    <%= flash_alert %>
    <main class="main">
      <%= yield %>
    </main>
  </body>
</html>
