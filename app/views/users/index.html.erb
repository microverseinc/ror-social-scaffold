<section class="users-section">
  <ul class="users-list">
    <% @users.each do |user| %>
      <li>
        Name: <%= user.name %>
        <span class="profile-link">
          <%= link_to 'See Profile',  user_path(user), class: 'profile-link' %>
          <%if !(@current_user.id==user.id)%>
          <% number = user.mutual?(@current_user, user) %>
          <%= link_to request_response(current_user.id,user.id), send_req_user_path(:id => user.id), class: "link"%>
          <p> number of mutual friends : <%= number %></p>
          <%end%> 
        </span>
      </li>
    <% end %>
  </ul>
</section>
