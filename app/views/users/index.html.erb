<% provide(:title, 'All users') %>
<h1>All users</h1>
<section class="users-section">
  <%= will_paginate %>
  <ul class="users-list">
    <% @users.each do |user| %>
      <li>
        <%= gravatar_for user, size: 50 %>
        <%= link_to user.name, user %>
      </li>
    <% end %>
  </ul>
  <%= will_paginate %>
</section>
