<section class="users-section">
  <ul class="users-list">
    <% @users.each do |user| %>
      <li class="list-group">
        Name: <%= user.name %>
        <span class="profile-link">
        <%= link_to 'See Profile',  user_path(user), class: 'profile-link' %>
        <%= friendship_stack(user) %>
      </li>
    <% end %>    
  </ul>
</section>
