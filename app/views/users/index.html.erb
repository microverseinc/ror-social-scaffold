<section class="users-section">
  <ul class="users-list">
    <% @users.each do |user| %>
      <li>
        Name: <%= user.name %>
        <span class="profile-link">
          <%= link_to 'See Profile',  user_path(user), class: 'profile-link' %>
        </span>
        <button type="button" name="button"> <%= link_to 'Add friend', new_user_friendship_path(user_id: current_user, friend_id: user.id), class: 'add_friend'  %></button>
      </li>
    <% end %>
  </ul>
</section>
