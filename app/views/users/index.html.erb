<section class="users-section">
  <ul class="users-list">
    <% @users.each do |user| %>
      <li>
        Name: <%= user.name %>
        <span class="profile-link">
          <%= link_to 'See Profile',  user_path(user), class: 'profile-link' %>
        
          <%if check_profile(user)%>
          <%= button_to "Add Friend",friendships_path(user_id:user)%>
          
          
          <%end%>
        </span>
        
      </li>
    <% end %>
  </ul>
</section>
